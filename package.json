{
  "name": "snowpack-react-ts",
  "version": "1.0.0",
  "description": "Snowpack React TS Project based on the \"@snowpack/app-template-react-typescript\" template using \"create-snowpack-app\"",
  "main": "",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build && workbox generateSW",
    "build-icon": "cd build && npx pwa-asset-generator ./dist/assets/logo.svg ./assets -i ./index.html -m ./manifest.json",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "preact": "^10.5.13",
    "preact-compat": "^3.19.0",
    "storeon": "^3.1.4",
    "styled-jsx": "^3.4.4"
  },
  "devDependencies": {
    "@prefresh/snowpack": "^3.1.2",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-typescript": "^1.2.0",
    "@snowpack/web-test-runner-plugin": "^0.2.0",
    "@types/node": "^15.0.1",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.12.0",
    "prettier": "^2.0.5",
    "pwa-asset-generator": "^4.1.1",
    "snowpack": "^3.0.1",
    "typescript": "^4.0.0",
    "workbox-cli": "^6.1.2"
  },
  "keywords": [
    "snowpack",
    "preact",
    "typescript"
  ]
}
